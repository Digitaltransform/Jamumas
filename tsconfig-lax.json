{
  "compilerOptions": {
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": ["dom", "es2018"],
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "noResolve": true,
    "preserveConstEnums": false,
    "skipLibCheck": true,
    "sourceMap": false,
    "target": "es5"
  }
}
